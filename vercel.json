{
  "rewrites": [{ "source": "/api/(.*)", "destination": "/api/db.js" }],
  "builds": [
    {
      "src": "api/db.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [{ "src": "/api/(.*)", "dest": "/api/db.js" }]
}
